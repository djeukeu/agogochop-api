{
  "name": "agogochop-api",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "node dist/server.js",
    "start:dev": "env-cmd -f .env.development nodemon",
    "build": "tsc && resolve-tspaths",
    "pretty": "prettier --write ./src/**/* ",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "dev": "env-cmd -f .env.development docker-compose up -d",
    "prisma:studio": "env-cmd -f .env.development prisma studio --port 5556",
    "prisma:deploy": "prisma migrate deploy",
    "prisma:dev": "env-cmd -f .env.development prisma migrate dev --name development",
    "prisma:prod": "env-cmd -f .env.development prisma migrate dev --name production",
    "prisma:reset": "prisma migrate reset"
  },
  "dependencies": {
    "@apollo/server": "^4.11.0",
    "@aws-sdk/client-s3": "^3.637.0",
    "@graphql-tools/schema": "^10.0.6",
    "@prisma/client": "5.16.1",
    "convict": "^6.2.4",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "fs-capacitor": "^8.0.0",
    "graphql": "^16.9.0",
    "graphql-middleware": "^6.1.35",
    "graphql-shield": "^7.6.5",
    "graphql-tag": "^2.12.6",
    "graphql-upload": "12.0.0",
    "uuid": "^10.0.0"
  },
  "devDependencies": {
    "@types/convict": "^6.1.6",
    "@types/cors": "^2.8.17",
    "@types/graphql-upload": "8.0.7",
    "@types/node": "^20.14.9",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "6.14.0",
    "@typescript-eslint/parser": "6.14.0",
    "env-cmd": "^10.1.0",
    "eslint": "8.56.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-import-resolver-typescript": "2.7.1",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-prettier": "4.2.0",
    "nodemon": "^3.1.4",
    "prettier": "2.8.1",
    "prisma": "^5.16.1",
    "resolve-tspaths": "^0.8.19",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "5.3.3"
  }
}
